<%= search_form_for @q, url: admin_means_path do |f| %>
  <div class="row">
    <div class="form-inline align-items-center mx-auto">
      <div class="col-auto">
        <%= f.search_field :title_cont, class: 'form-control', placeholder: "タイトル名" %>
      </div>
      <div class="col-auto">
        <%= f.search_field :user_name_cont, class: 'form-control', placeholder: "ユーザー名" %>
      </div>
      <div class="col-auto">
        <%= f.submit class: 'btn btn-primary' %>
      </div>
    </div>
  </div>
<% end %>